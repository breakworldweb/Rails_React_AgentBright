<div class="row">
  <div class="col-xs-12">
    <h3 class="inline-block">
      <a data-toggle="collapse" class="clickable" data-toggle-event="true" data-target="#client-info">
        <div class="title-arrow fui-arrow-right"></div>User Info
      </a>
    </h3>
    <div id="client-info" class="collapse in">
      <ul class="dotted-list">
        <li class="clearfix">
          <div class="float-left col-sm-4 fwb dim-el">User Name</div>
          <div class="col-sm-8"><%= link_to @user.name, @user.name %></div>
        </li>
        <li class="clearfix">
          <div class="float-left col-sm-4 fwb dim-el">First Name</div>
          <div class="col-sm-8"><%= @user.first_name %></div>
        </li>
                <li class="clearfix">
          <div class="float-left col-sm-4 fwb dim-el">Last Name</div>
          <div class="col-sm-8"><%= "#{@user.last_name}" %></div>
        </li>
        <li class="clearfix">
          <div class="float-left col-sm-4 fwb dim-el">Phone</div>
          <div class="col-sm-8"><%= @user.mobile_number %></div>
        </li>
        <li class="clearfix">
          <div class="float-left col-sm-4 fwb dim-el">Office Number</div>
          <div class="col-sm-8"><%= @user.fax_number %></div>
        </li>
        <li class="clearfix">
          <div class="float-left col-sm-4 fwb dim-el">Email</div>
          <div class="col-sm-8"><%= @user.email %></div>
        </li>
        <li class="clearfix">
          <div class="float-left col-sm-4 fwb dim-el">Address</div>
          <div class="col-sm-8">
            <div class="inline-block">
              <% @user.address %>
              <%= @user.city%> <br/>
              <%= @user.state %><%= @user.zip %>
            </div>
          </div>
        </li>
      </ul>
    </div><!-- /#userinfo.collapse in -->
  </div><!-- /.col-xs-12 -->
</div><!-- /.row -->
