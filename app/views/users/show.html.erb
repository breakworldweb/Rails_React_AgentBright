      <%= link_to "Users", users_path %>
    </h1>
    <h2><%= @user.name %></h2>
    <div class="row">
      <div class="col-sm-12">


      </div><!-- /col-sm-12 -->
    </div><!-- /row -->

  </div><!-- /.col-xs-12 -->

