<!--HEADER SECTION--><!--BASIC TABLE-->
<div class="row">
  <div class="lastUnit col-sm-12 header">
    <h1 class="inline-block">
      <%= link_to "Home", root_path %>
    </h1>
    <h2>Users</h2>
  </div>
</div>

<!--BASIC TABLE-->

<div class="row below30">
  <div class="col-xs-12">
    <h3>List of Users</h3>
    <table class="ab-table">
      <thead>
      <tr>
        <th width="30%"> Agent Name</th>
        <th width="30%"> Email</th>
        <th width="20%"> Member Since</th>
        <th width="20%"> Billing Plan</th>
        <th width="20%"> Last Active</th>
      </tr>
      </thead>
      <tbody>

      <% @users.each do |user| %>
          <tr>
            <td class="breakword">
              <h4 class="nomargin"><%= link_to user.name, user %></h4>
            </td>
            <td>
              <%=  user.email  %>
            </td>
            <td>
              <%= '9/9/2013 (holding)' %>
            </td>
            <td>
              <%= 'Pilot Programme (holding)' %>
            </td>            <td>
              <%= '10/19/2014 (holding)' %>
            </td>

          </tr>
        <% end %>

      </tbody>

    </table>

  </div>

</div>


